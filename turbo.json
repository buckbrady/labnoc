{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "SUPERTOKENS_API_KEY",
    "LABNOC_API_RATE_LIMIT_GLOBAL"
  ]
}
