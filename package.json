{
  "name": "labnoc",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "eslint --fix \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": ">=8.0.1 <9.0.0",
    "eslint-config-custom": "workspace:*",
    "ts-node-dev": "^2.0.0",
    "turbo": "latest",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.24.2",
  "peerDependencies": {
    "eslint": ">=8.0.1 <9.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "mongoose": "^6.8.3"
  }
}